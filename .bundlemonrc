{
    "baseDir": "./build",
    "groups": [
        {
            "path": "./**/*",
            "friendlyName": "Build Folder",
            "compression": "gzip",
            "maxSize": "2.5 MB",
            "maxPercentIncrease": "20"
        }
    ],
    "reportOutput": [
        [
            "github",
            {
                "checkRun": true,
                "commitStatus": true,
                "prComment": true
            }
        ]
    ]
}